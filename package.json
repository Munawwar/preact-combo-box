{
  "name": "preact-combobox",
  "version": "0.0.1",
  "description": "Multiselect Combo box component for preact",
  "main": "dist/cjs/MultiSelectAutocomplete.js",
  "module": "dist/esm/MultiSelectAutocomplete.js",
  "scripts": {
    "dev": "concurrently -i -c \"auto\" \"npm:watch\" \"npm:example\"",
    "prepare": "npm run build",
    "prepublish": "npm run build",
    "lint": "npx @biomejs/biome check --write",
    "pretest": "npm run build",
    "example": "browser-sync start --port 3050 --server --files 'example' --files 'dist' --open './example/example-react.html'",
    "watch": "esbuild ./lib/*.jsx --outdir=dist/esm --format=esm --bundle --sourcemap --external:preact --external:@popperjs/core --watch",
    "build": "rm -fr dist && esbuild ./lib/*.jsx --outdir=dist/esm --format=esm --bundle --sourcemap --external:preact --external:@popperjs/core && esbuild ./lib/*.jsx --outdir=dist/cjs --format=cjs --bundle --sourcemap --external:react"
  },
  "keywords": [
    "preact",
    "tags",
    "tag input",
    "component",
    "multiselect",
    "combobox",
    "autocomplete"
  ],
  "author": "Munawwar",
  "license": "MIT",
  "repository": "https://github.com/Munawwar/preact-multiselect",
  "devDependencies": {
    "@biomejs/biome": "1.8.3",
    "@popperjs/core": "^2",
    "browser-sync": "^3.0.2",
    "concurrently": "^9.1.2",
    "esbuild": "^0.21.5",
    "preact": "^10.25.4",
    "react": "^16 || ^17 || ^18",
    "react-dom": "^16 || ^17 || ^18",
    "typescript": "^5.5.3"
  },
  "peerDependencies": {
    "@popperjs/core": "^2",
    "react": "^16 || ^17 || ^18",
    "react-dom": "^16 || ^17 || ^18"
  },
  "engines": {
    "node": ">= 10.0.0"
  }
}
